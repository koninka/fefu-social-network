{% extends "NetworkWebBundle:Default:profile.base.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('bundles/networkweb/css/video.css') }}" type="text/css" rel="stylesheet" />
{% endblock %}
{% block right_content_sub_one %}
    <div id="video_box">
        <form action="{{ path('edit_video', {'id': video.id}) }}" {{ form_enctype(form) }} method="POST">
            {{ form_widget(form) }}
            <div>
                <input class="blue_button" type="submit" value="{{'video.edit.submit'|trans }}" />
            </div>
        </form>
    </div>
{% endblock %}
