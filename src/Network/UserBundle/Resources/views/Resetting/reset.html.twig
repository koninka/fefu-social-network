{% extends "NetworkWebBundle:Default:auth.base.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}
{% block title %}
    {{ 'resetting.password_already_requested'|trans }}
{% endblock %}
{% block authorization %}
    <form  class="auth-form" action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST">
        {{ form_widget(form) }}
        <div>
            <input class="submit_button" type="submit" value="{{ 'resetting.reset.submit'|trans }}" />
        </div>
    </form>
{% endblock authorization %}
